{
  "expo": {
    "name": "NexHire",
    "slug": "nexhire",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#3B82F6"
    },
    "scheme": "nexhire",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nexhire.app"
    },
    "android": {
      "package": "com.nexhire.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "nexhire.com"
            },
            {
              "scheme": "https", 
              "host": "www.nexhire.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": []
  }
}